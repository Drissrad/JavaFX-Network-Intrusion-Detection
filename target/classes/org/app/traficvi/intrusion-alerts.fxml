<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="481.0" prefWidth="334.0" spacing="10" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21.0.5">
    <Label style="-fx-font-size: 18px;" text="Détails des Alertes d'Intrusion" />
    <TableView fx:id="alertTable" prefHeight="408.0" prefWidth="274.0">
        <columns>
            <TableColumn fx:id="typeColumn" text="Type" />
            <TableColumn fx:id="detailsColumn" text="Détails" />
            <TableColumn fx:id="timestampColumn" text="Horodatage" />
        </columns>
    </TableView>
    <HBox alignment="CENTER_RIGHT">
        <Button fx:id="raportCsv" text="Raport CSV" />
        <Button fx:id="raportPdf" text="Raport Pdf" />
        <Button fx:id="backButton" text="Retour" />
    </HBox>

</VBox>
